e() {\r\n          axios.get('https://localhost:44378/api/documento/coche')\r\n              .then(response => {\r\n                  if (response.data && Array.isArray(response.data.data)) {\r\n                      this.coches = response.data.data;\r\n                      console.log('Datos de coches cargados correctamente:', this.coches);\r\n                  } else {\r\n                      console.error('La respuesta no contiene la estructura esperada');\r\n                  }\r\n              })\r\n              .catch(error => {\r\n                  console.error('Error al obtener los datos de coches desde MongoDB', error);\r\n              });\r\n      },\r\n      close() {\r\n          this.dialog = false;\r\n      },\r\n      closeDelete() {\r\n          this.dialogDelete = false;\r\n      },\r\n      async save() {\r\n          try {\r\n              if (this.editedIndex > -1) {\r\n                  await axios.put(`https://localhost:44378/api/documento/coche/actualizacion/${this.editedItem.id}`, this.editedItem);\r\n              } else {\r\n                  await axios.post('https://localhost:44378/api/documento/coche/registro', this.editedItem);\r\n              }\r\n\r\n              this.initialize();\r\n\r\n              this.close();\r\n          } catch (error) {\r\n              console.error('Error al guardar los datos en MongoDB:', error);\r\n          }\r\n      },\r\n      async deleteItemConfirm() {\r\n          try {\r\n              if (this.editedIndex > -1) {\r\n                  await axios.delete(`https://localhost:44378/api/documento/coche/${this.editedItem.id}`);\r\n                  this.initialize();\r\n              }\r\n              this.closeDelete();\r\n          } catch (error) {\r\n              console.error('Error al eliminar el elemento en MongoDB:', error);\r\n          }\r\n      },\r\n      editItem(item) {\r\n          this.formTitle = 'Editar';\r\n          this.editedItem = { ...item };\r\n          this.editedIndex = this.coches.indexOf(item);\r\n          this.dialog = true;\r\n      },\r\n      deleteItem(item) {\r\n          this.editedIndex = this.coches.indexOf(item);\r\n          this.editedItem = Object.assign({}, item);\r\n          this.dialogDelete = true;\r\n      },\r\n  },\r\n  watch: {\r\n      dialog(val) {\r\n          val || this.close();\r\n      },\r\n      dialogDelete(val) {\r\n          val || this.closeDelete();\r\n      },\r\n  },\r\n  created() {\r\n      this.initialize;\r\n  },\r\n\r\n}\r\n</script>"],"mappings":"AAsFA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,kBAAA;MACAC,MAAA;MACAC,YAAA;MACAC,OAAA,GACA;QACAC,IAAA;QACAC,KAAA;QACAC,QAAA;QACAC,KAAA;MACA,GACA;QAAAH,IAAA;QAAAG,KAAA;MAAA,GACA;QAAAH,IAAA;QAAAG,KAAA;MAAA,GACA;QAAAH,IAAA;QAAAG,KAAA;MAAA,GACA;QAAAH,IAAA;QAAAG,KAAA;MAAA,GACA;QAAAH,IAAA;QAAAG,KAAA;QAAAD,QAAA;MAAA,EACA;MACAE,MAAA;MACAC,WAAA;MACAC,UAAA;QACAC,KAAA;QACAC,MAAA;QACAC,GAAA;QACAC,KAAA;QACAC,MAAA;MACA;MACAC,WAAA;QACAL,KAAA;QACAC,MAAA;QACAC,GAAA;QACAC,KAAA;QACAC,MAAA;MACA;MACAE,SAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACAtB,KAAA,CAAAuB,GAAA,gDACAC,IAAA,CAAAC,QAAA;QACA,IAAAA,QAAA,CAAAvB,IAAA,IAAAwB,KAAA,CAAAC,OAAA,CAAAF,QAAA,CAAAvB,IAAA,CAAAA,IAAA;UACA,KAAAS,MAAA,GAAAc,QAAA,CAAAvB,IAAA,CAAAA,IAAA;UACA0B,OAAA,CAAAC,GAAA,iDAAAlB,MAAA;QACA;UACAiB,OAAA,CAAAE,KAAA;QACA;MACA,GACAC,KAAA,CAAAD,KAAA;QACAF,OAAA,CAAAE,KAAA,uDAAAA,KAAA;MACA;IACA;IACAE,MAAA;MACA,KAAA5B,MAAA;IACA;IACA6B,YAAA;MACA,KAAA5B,YAAA;IACA;IACA,MAAA6B,KAAA;MACA;QACA,SAAAtB,WAAA;UACA,MAAAZ,KAAA,CAAAmC,GAAA,mEAAAtB,UAAA,CAAAuB,EAAA,SAAAvB,UAAA;QACA;UACA,MAAAb,KAAA,CAAAqC,IAAA,8DAAAxB,UAAA;QACA;QAEA,KAAAS,UAAA;QAEA,KAAAU,KAAA;MACA,SAAAF,KAAA;QACAF,OAAA,CAAAE,KAAA,2CAAAA,KAAA;MACA;IACA;IACA,MAAAQ,kBAAA;MACA;QACA,SAAA1B,WAAA;UACA,MAAAZ,KAAA,CAAAuC,MAAA,qDAAA1B,UAAA,CAAAuB,EAAA;UACA,KAAAd,UAAA;QACA;QACA,KAAAW,WAAA;MACA,SAAAH,KAAA;QACAF,OAAA,CAAAE,KAAA,8CAAAA,KAAA;MACA;IACA;IACAU,SAAAC,IAAA;MACA,KAAArB,SAAA;MACA,KAAAP,UAAA;QAAA,GAAA4B;MAAA;MACA,KAAA7B,WAAA,QAAAD,MAAA,CAAA+B,OAA