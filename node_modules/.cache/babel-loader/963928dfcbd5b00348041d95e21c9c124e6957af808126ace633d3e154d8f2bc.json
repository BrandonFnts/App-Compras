bsp; Login\r\n                            </v-btn>\r\n                        </v-card-actions>\r\n\r\n                    </form>\r\n                    <!-- Agregamos v-alerts aquí -->\r\n                    <v-alert v-if=\"showSuccessAlert\" type=\"success\" title=\"Bienvenido\"></v-alert>\r\n                    <v-alert v-if=\"showWarningAlert\" type=\"warning\" title=\"Usuario o contraseña incorrectos\"></v-alert>\r\n                    <v-alert v-if=\"showErrorAlert\" type=\"error\" title=\"Error al iniciar sesión\"></v-alert>\r\n                </v-card>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nimport { mapMutations } from 'vuex';\r\n\r\nexport default {\r\n    name: 'LoginComponent',\r\n    data() {\r\n        return {\r\n            usuario: {\r\n                userName: '',\r\n                password: '',\r\n            },\r\n            showSuccessAlert: false,\r\n            showWarningAlert: false,\r\n            showErrorAlert: false,\r\n        };\r\n    },\r\n    methods: {\r\n        ...mapMutations(['setUserName']),\r\n        async validateUser() {\r\n            try {\r\n                const response = await axios.get('https://localhost:44331/api/documento/login', {\r\n                    params: {\r\n                        username: this.usuario.userName,\r\n                        password: this.usuario.password,\r\n                    },\r\n                });\r\n\r\n                if (response.data && response.data.status_code === 200) {\r\n                    const users = response.data.data;\r\n\r\n                    const matchingUser = users.find(\r\n                        (user) => user.nombre_usuario === this.usuario.userName && user.password === this.usuario.password\r\n                    );\r\n\r\n                    if (matchingUser) {\r\n                        this.setUserName(matchingUser.nombre_usuario);\r\n                        localStorage.setItem('userName', this.usuario.userName);\r\n                        this.showSuccessAlert = true;\r\n                        this.showWarningAlert = false;\r\n                        this.showErrorAlert = false;\r\n                        this.$router.push('/manager-proveedores');\r\n                    } else {\r\n                        this.showSuccessAlert = false;\r\n                        this.showWarningAlert = true;\r\n                        this.showErrorAlert = false;\r\n                    }\r\n                } else {\r\n                    this.showSuccessAlert = false;\r\n                    this.showWarningAlert = false;\r\n                    this.showErrorAlert = true;\r\n                }\r\n            } catch (error) {\r\n                console.error('Error al autenticar usuario', error);\r\n                alert('Error al autenticar usuario. Por favor, inténtalo de nuevo.');\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n  \r\n<style>\r\n.nav-color {\r\n    background-color: orange !important;\r\n}\r\n\r\n.title-nav {\r\n    color: white !important;\r\n    font-family: 'Roboto', 'sans-serif';\r\n    font-weight: 400;\r\n}\r\n\r\n.title-nav-bar-text {\r\n    font-family: 'Roboto', 'sans-serif';\r\n    font-weight: 300;\r\n}\r\n</style>\r\n  "],"mappings":";AAwCA,OAAAA,KAAA;AACA,SAAAC,YAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;QACAC,QAAA;QACAC,QAAA;MACA;MACAC,gBAAA;MACAC,gBAAA;MACAC,cAAA;IACA;EACA;EACAC,OAAA;IACA,GAAAT,YAAA;IACA,MAAAU,aAAA;MACA;QACA,MAAAC,QAAA,SAAAZ,KAAA,CAAAa,GAAA;UACAC,MAAA;YACAC,QAAA,OAAAX,OAAA,CAAAC,QAAA;YACAC,QAAA,OAAAF,OAAA,CAAAE;UACA;QACA;QAEA,IAAAM,QAAA,CAAAT,IAAA,IAAAS,QAAA,CAAAT,IAAA,CAAAa,WAAA;UACA,MAAAC,KAAA,GAAAL,QAAA,CAAAT,IAAA,CAAAA,IAAA;UAEA,MAAAe,YAAA,GAAAD,KAAA,CAAAE,IAAA,CACAC,IAAA,IAAAA,IAAA,CAAAC,cAAA,UAAAjB,OAAA,CAAAC,QAAA,IAAAe,IAAA,CAAAd,QAAA,UAAAF,OAAA,CAAAE,QACA;UAEA,IAAAY,YAAA;YACA,KAAAI,WAAA,CAAAJ,YAAA,CAAAG,cAAA;YACAE,YAAA,CAAAC,OAAA,kBAAApB,OAAA,CAAAC,QAAA;YACA,KAAAE,gBAAA;Y