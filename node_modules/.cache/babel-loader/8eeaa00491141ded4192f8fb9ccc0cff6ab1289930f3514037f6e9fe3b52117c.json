{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'LoginComponent',\n  data() {\n    return {\n      usuario: {\n        userName: \"\",\n        password: \"\"\n      }\n    };\n  },\n  methods: {\n    async validateUser() {\n      try {\n        const response = await axios.get('https://localhost:44331/api/documento/login', {\n          username: this.usuario.userName,\n          password: this.usuario.password\n        });\n        // Verifica el código de estado de la respuesta\n        if (response.data.status_code === 200) {\n          // La autenticación fue exitosa\n          const userData = response.data.data[0]; // Aquí asumí que solo hay un usuario en la respuesta\n          console.log('Usuario autenticado:', userData);\n\n          // Puedes realizar acciones adicionales según la respuesta del servidor\n        } else {\n          // La autenticación falló\n          console.error('Error al autenticar usuario:', response.data.message);\n\n          // Puedes mostrar un mensaje de error al usuario\n          alert('Error al autenticar usuario. Por favor, inténtalo de nuevo.');\n        }\n      } catch (error) {\n        console.error('Error al autenticar usuario', error);\n        alert('Error al autenticar usuario. Por favor, inténtalo de nuevo.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","usuario","userName","password","methods","validateUser","response","get","username","status_code","userData","console","log","error","message","alert"],"sources":["src/components/autenticacion/LoginComponent.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\">\r\n        <div class=\"row\">\r\n            <div class=\"col-xs-12 col-sm-12 col-md-8 col-lg-8\">\r\n                <v-card elevation=\"4\" width=\"500\" class=\"mx-auto justify-md-center float-right\">\r\n                    <v-toolbar dark class=\"nav-color\">\r\n                        <v-toolbar-title class=\"title-nav-bar-text\">\r\n                            <i class=\"fas fa-user-lock\"></i> &nbsp;\r\n                            ¡Bienvenido!</v-toolbar-title>\r\n                    </v-toolbar>\r\n                    <form @submit.prevent=\"handleform\">\r\n                        <v-card-text>\r\n                            <v-text-field label=\"Nombre de Usuario\" outlined v-model=\"usuario.userName\" color=\"orange\"\r\n                                clearable dense class=\"pa-1\">\r\n                            </v-text-field>\r\n                            <div class=\"text--primary\">\r\n                                <v-text-field label=\"Password\" v-model=\"usuario.password\" outlined color=\"orange\" clearable\r\n                                    dense class=\"pa-1\" type=\"password\">\r\n                                </v-text-field>\r\n                            </div>\r\n                        </v-card-text>\r\n                        <v-card-actions>\r\n                            <v-btn outlined class=\"ma-1 pa-4 ml-auto\" color=\"orange\" @click=\"validateUser\">\r\n                                <i class=\"fas fa-sign-in-alt\"></i>&nbsp; Login\r\n                            </v-btn>\r\n                        </v-card-actions>\r\n                    </form>\r\n                </v-card>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<style>\r\n.nav-color {\r\n    background-color: orange !important;\r\n}\r\n\r\n.title-nav {\r\n    color: white !important;\r\n    font-family: \"Roboto\", \"sans-serif\";\r\n    font-weight: 400;\r\n}\r\n\r\n.title-nav-bar-text {\r\n    font-family: \"Roboto\", \"sans-serif\";\r\n    font-weight: 300;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n    name: 'LoginComponent',\r\n    data() {\r\n        return {\r\n            usuario: {\r\n                userName: \"\",\r\n                password: \"\",\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        async validateUser() {\r\n            try {\r\n                const response = await axios.get('https://localhost:44331/api/documento/login', {\r\n                    username: this.usuario.userName,\r\n                    password: this.usuario.password,\r\n                });\r\n                // Verifica el código de estado de la respuesta\r\n                if (response.data.status_code === 200) {\r\n                    // La autenticación fue exitosa\r\n                    const userData = response.data.data[0];  // Aquí asumí que solo hay un usuario en la respuesta\r\n                    console.log('Usuario autenticado:', userData);\r\n\r\n                    // Puedes realizar acciones adicionales según la respuesta del servidor\r\n                } else {\r\n                    // La autenticación falló\r\n                    console.error('Error al autenticar usuario:', response.data.message);\r\n\r\n                    // Puedes mostrar un mensaje de error al usuario\r\n                    alert('Error al autenticar usuario. Por favor, inténtalo de nuevo.');\r\n                }\r\n            } catch (error) {\r\n                console.error('Error al autenticar usuario', error);\r\n                alert('Error al autenticar usuario. Por favor, inténtalo de nuevo.');\r\n            }\r\n        },\r\n    },\r\n}\r\n</script>"],"mappings":"AAmDA,OAAAA,KAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;QACAC,QAAA;QACAC,QAAA;MACA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,aAAA;MACA;QACA,MAAAC,QAAA,SAAAR,KAAA,CAAAS,GAAA;UACAC,QAAA,OAAAP,OAAA,CAAAC,QAAA;UACAC,QAAA,OAAAF,OAAA,CAAAE;QACA;QACA;QACA,IAAAG,QAAA,CAAAN,IAAA,CAAAS,WAAA;UACA;UACA,MAAAC,QAAA,GAAAJ,QAAA,CAAAN,IAAA,CAAAA,IAAA;UACAW,OAAA,CAAAC,GAAA,yBAAAF,QAAA;;UAEA;QACA;UACA;UACAC,OAAA,CAAAE,KAAA,iCAAAP,QAAA,CAAAN,IAAA,CAAAc,OAAA;;UAEA;UACAC,KAAA;QACA;MACA,SAAAF,KAAA;QACAF,OAAA,CAAAE,KAAA,gCAAAA,KAAA;QACAE,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}