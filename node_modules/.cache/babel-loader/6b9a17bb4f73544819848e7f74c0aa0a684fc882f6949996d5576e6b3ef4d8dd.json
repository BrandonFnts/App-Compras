;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { mapMutations } from 'vuex';\r\n\r\nexport default {\r\n    name: 'SiginComponent',\r\n    data() {\r\n        return {\r\n            usuario: {\r\n                userName: \"\",\r\n                password: \"\",\r\n            },\r\n            showSuccessAlert: false,\r\n            showErrorAlert: false,\r\n        }\r\n    },\r\n    methods: {\r\n        ...mapMutations(['setUserName']),\r\n        async registerUser() {\r\n            try {\r\n                const response = await axios.post('https://localhost:44331/api/documento/login/registro', {\r\n                    username: this.usuario.userName,\r\n                    password: this.usuario.password,\r\n                });\r\n\r\n                if (response.data && response.data.status_code === 200) {\r\n                    this.setUserName(this.usuario.userName);\r\n                    localStorage.setItem('userName', this.usuario.userName);\r\n\r\n                    this.showSuccessAlert = true;\r\n\r\n                    setTimeout(() => {\r\n                        this.$router.push('/login');\r\n                    }, 2000);\r\n                } else {\r\n                    this.showErrorAlert = true;\r\n\r\n                    alert('Error en el registro. Por favor, inténtalo de nuevo.');\r\n                }\r\n            } catch (error) {\r\n                console.error('Error al registrar usuario', error);\r\n                alert('Error al registrar usuario. Por favor, inténtalo de nuevo.');\r\n            }\r\n        },\r\n    },\r\n}\r\n</script>"],"mappings":";AAsDA,OAAAA,KAAA;AACA,SAAAC,YAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;QACAC,QAAA;QACAC,QAAA;MACA;MACAC,gBAAA;MACAC,cAAA;IACA;EACA;EACAC,OAAA;IACA,GAAAR,YAAA;IACA,MAAAS,aAAA;MACA;QACA,MAAAC,QAAA,SAAAX,KAAA,CAAAY,IAAA;UACAC,QAAA,OAAAT,OAAA,CAAAC,QAAA;UACAC,QAAA,OAAAF,OAAA,CAAAE;QACA;QAEA,IAAAK,QAAA,CAAAR,IAAA,IAAAQ,QAAA,CAAAR,IAAA,CAAAW,WAAA;UACA